<head>

    <!-- //Meta Tags -->
    <!-- Style-sheets -->
    <!-- Bootstrap Css -->
    <link href="/tables/css/bootstrap.css" rel="stylesheet" type="text" @*media="all"*@ />
    <!-- Bootstrap Css -->
    <!-- Common Css -->
    <link href="/tables/css/style.css" rel="stylesheet" type="text/css" @*media="all"*@ />
    <!--// Common Css -->
    <!-- Nav Css -->
    @*<link rel="stylesheet" href="/tables/css/style4.css">*@
    <!--// Nav Css -->
