    <!--//web-fonts-->

</head>
<h2 style="font-family:Roboto">Driver Delivery</h2>
<body>

    <div class="outer-w3-agile mt-3">
        <h4 class="tittle-w3-agileits mb-4"></h4>
        <table class="table table-bordered table-responsive ">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">  @Html.DisplayNameFor(model => model.OrderID)</th>
                    <th scope="col">  @Html.DisplayNameFor(model => model.DeliveryAddress)</th>
                    <th scope="col">  @Html.DisplayNameFor(model => model.Name)</th>
                    <th scope="col">  @Html.DisplayNameFor(model => model.Email)</th>
                    <th scope="col">  @Html.DisplayNameFor(model => model.CustomerPhone)</th>
                    <th scope="col">  @Html.DisplayNameFor(model => model.DriverID)</th>
                    <th></th>
                   


                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <th scope="row"> @item.OrderID</th>
                    <td>  @item.DeliveryAddress</td>
                    <td>  @item.Name</td>
                    <td>  @item.Email</td>
                    <td>  @item.CustomerPhone</td>
                    <td>  @item.DriverID</td>
                    <td>
                        <a href='@Url.Action( "CusDetails", "Order", new {id = item.OrderID})' class="btn btn-primary">Details</a>
                      

                    </td>


                </tr>

                }
            </tbody>
        </table>


    </div>
</body>




